<?xml version="1.0" encoding="utf-8"?>
  <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#ffffff"
    android:clickable="false"  >

     <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="45dp"
        android:background="@drawable/title_bar_blue"
        android:gravity="center_vertical"
        android:clickable="false" >
	<RelativeLayout 
	    android:layout_width="70dp"
	    android:layout_height="match_parent"
	    android:layout_alignParentLeft="true"
	    android:onClick="fh"
	    android:layout_centerVertical="true">
    <Button
        android:onClick="fh"
        android:layout_width="14dp"
        android:layout_height="24dp"
        android:layout_centerVertical="true"
        android:background="@drawable/title_btn_back_blue"
        android:layout_marginLeft="17dp"
         />
</RelativeLayout>
<TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="续报事件信息"
            android:textColor="#ffffff"
            android:textSize="20sp" />
</RelativeLayout>
        
    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent" 
        android:layout_marginTop="45dp"
        android:clickable="false" >
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

          <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:background="#ffffff"
            android:clickable="false" >

            <include layout="@layout/line"/>  
	    	<!-- 续报名称-->
           <RelativeLayout
              android:layout_width="match_parent"
              android:layout_height="50dp"  > 
                <TextView 
                    android:layout_width="80dp"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:text="续报名称"
                    android:layout_marginLeft="10dp"
                    style="@style/title_text"/>
                <TextView 
                    android:id="@+id/continue_name"
                    android:layout_width="220dp"
                    android:layout_height="50dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true"
                    android:gravity="center_vertical|left"
                    style="@style/medium_text"
                    android:textSize="15sp"
                    android:background="#ffffff"
                    android:layout_marginLeft="90dp"
                    />
            </RelativeLayout>
            <include layout="@layout/line_mlr"/>  
	    	<!--续报类别  -->
           <RelativeLayout
              android:layout_width="match_parent"
              android:layout_height="50dp"
                > 
                <TextView 
                    android:layout_width="80dp"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:text="续报类别"
                    android:layout_marginLeft="10dp"
                    style="@style/title_text"/>
                <TextView
                    android:id="@+id/continue_type"
                    android:layout_width="220dp"
                    android:layout_height="50dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true"
                    android:gravity="center_vertical|left"
                    style="@style/medium_text"
                    android:background="#ffffff"
                    android:layout_marginLeft="90dp"
                    android:textSize="15sp"
                    />
             
            </RelativeLayout>
            <include layout="@layout/line_mlr"/>  
	    	<!--所属区域-->
           <RelativeLayout
              android:layout_width="match_parent"
              android:layout_height="50dp"
                > 
                <TextView 
                    android:layout_width="80dp"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:text="所属区域"
                    android:layout_marginLeft="10dp"
                    style="@style/title_text"/>
                <TextView 
                    android:id="@+id/continue_area"
                    android:layout_width="220dp"
                    android:layout_height="50dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true"
                    android:gravity="center_vertical|left"
                    style="@style/medium_text"
                    android:background="#ffffff"
                    android:layout_marginLeft="90dp"
                    android:textSize="15sp"
                    />
            </RelativeLayout>
            <include layout="@layout/line_mlr"/>  
            <!--发生地点-->

           <RelativeLayout
               android:layout_width="match_parent"
               android:layout_height="50dp" >

               <TextView
                   style="@style/title_text"
                   android:layout_width="80dp"
                   android:layout_height="match_parent"
                   android:layout_marginLeft="10dp"
                   android:gravity="center_vertical"
                   android:text="发生地点" />

               <TextView
                   android:id="@+id/continue_addr"
                   style="@style/medium_text"
                   android:layout_width="220dp"
                   android:layout_height="50dp"
                   android:layout_alignParentLeft="true"
                   android:layout_centerVertical="true"
                   android:layout_marginLeft="90dp"
                   android:background="#ffffff"
                   android:gravity="center_vertical|left"
                   android:textSize="15sp" />             
           </RelativeLayout>

            <include layout="@layout/line_mlr"/>  
           <!--续报时间-->
           
           <RelativeLayout
              android:layout_width="match_parent"
              android:layout_height="50dp"
                > 
                <TextView 
                    android:layout_width="80dp"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:text="续报时间"
                    android:layout_marginLeft="10dp"
                    style="@style/title_text"/>
                <TextView 
                    android:id="@+id/continue_time"
                    android:layout_width="220dp"
                    android:layout_height="50dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true"
                    android:gravity="center_vertical|left"
                    style="@style/medium_text"
                    android:background="#ffffff"
                    android:layout_marginLeft="90dp"
                    android:textSize="15sp"
                    />
            </RelativeLayout>
            <include layout="@layout/line_mlr"/>
            <!--续报单位-->
           <RelativeLayout
              android:layout_width="match_parent"
              android:layout_height="50dp"
                > 
                <TextView 
                    android:layout_width="80dp"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:text="续报单位"
                    android:layout_marginLeft="10dp"
                    style="@style/title_text"/>
                <TextView 
                    android:id="@+id/continue_dept"
                    android:layout_width="220dp"
                    android:layout_height="50dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true"
                    android:gravity="center_vertical|left"
                    style="@style/medium_text"
                    android:background="#ffffff"
                    android:layout_marginLeft="90dp"
                    android:textSize="15sp"
                    />
            </RelativeLayout>
            <include layout="@layout/line_mlr"/>  
            <!--事件报告人-->
           <RelativeLayout
              android:layout_width="match_parent"
              android:layout_height="50dp"
                > 
                <TextView 
                    android:layout_width="80dp"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:text="事件报告人"
                    android:layout_marginLeft="10dp"
                    style="@style/title_text"/>
                <TextView 
                    android:id="@+id/continue_reporter"
                    android:layout_width="220dp"
                    android:layout_height="50dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true"
                    android:gravity="center_vertical|left"
                    style="@style/medium_text"
                    android:background="#ffffff"
                    android:layout_marginLeft="90dp"
                    android:textSize="15sp"
                    />
            </RelativeLayout>
            <include layout="@layout/line_mlr"/>  
            <!--事件报告人电话-->
           <RelativeLayout
              android:layout_width="match_parent"
              android:layout_height="50dp"
                > 
                <TextView 
                    android:layout_width="80dp"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:text="事件报告人电话"
                    android:layout_marginLeft="10dp"
                    style="@style/title_text"/>
                <TextView 
                    android:id="@+id/continue_reportertel"
                    android:layout_width="220dp"
                    android:layout_height="50dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true"
                    android:gravity="center_vertical|left"
                    style="@style/medium_text"
                    android:background="#ffffff"
                    android:layout_marginLeft="90dp"
                    android:textSize="15sp"
                    />
            </RelativeLayout>
            <include layout="@layout/line_mlr"/>  
	    	<!-- 续报内容-->
           <RelativeLayout
              android:layout_width="match_parent"
              android:layout_height="90dp" > 
                <TextView 
                    android:layout_width="80dp"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:text="续报内容"
                    android:layout_marginLeft="10dp"
                    style="@style/title_text"/>
                <TextView 
                    android:id="@+id/continue_content"
                    android:layout_width="220dp"
                    android:layout_height="80dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true" 
                    android:gravity="center_vertical|left"
                    style="@style/medium_text" 
                    android:background="#ffffff"
                    android:layout_marginLeft="90dp" 
                    />
            </RelativeLayout>
 			<include layout="@layout/line_mlr"/>
	    	<!-- 采取措施-->
           <RelativeLayout
              android:layout_width="match_parent"
              android:layout_height="90dp" > 
                <TextView 
                    android:layout_width="80dp"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:text="采取措施"
                    android:layout_marginLeft="10dp"
                    style="@style/title_text"/>
                <TextView 
                    android:id="@+id/continue_step"
                    android:layout_width="220dp"
                    android:layout_height="80dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true" 
                    android:gravity="center_vertical|left"
                    style="@style/medium_text" 
                    android:background="#ffffff"
                    android:layout_marginLeft="90dp" 
                    />
            </RelativeLayout>
            <!--事件指标  -->
	    	<include layout="@layout/line_mlr"/>
           <RelativeLayout
              android:layout_width="match_parent"
              android:layout_height="90dp" > 
                <TextView 
                    android:layout_width="80dp"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:text="事件指标"
                    android:layout_marginLeft="10dp"
                    style="@style/title_text"/>
                <TextView 
                    android:id="@+id/continue_monitor"
                    android:layout_width="220dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true" 
                    android:gravity="center_vertical|left"
                    style="@style/medium_text" 
                    android:background="#ffffff"
                    android:layout_marginLeft="90dp" 
                    />
            </RelativeLayout>
             <include layout="@layout/line"/> 
             <!-- 附件 -->
			<LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="130dp" 
                android:layout_marginTop="14dp" 
                android:background="#ffffff" 
                android:orientation="vertical" 
                android:clickable="false" > 
                <include layout="@layout/line"/> 
                <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="129dp" 
                android:clickable="false"     >
                    <net.evecom.androidecssp.view.gallery.GalleryFlow
				        android:id="@+id/continue_gallery_flow"
				        android:layout_width="match_parent"
				        android:layout_height="match_parent" /> 
                </RelativeLayout>
                <include layout="@layout/line"/> 
                </LinearLayout>
            </LinearLayout>
            
            </LinearLayout>
          </ScrollView>

  

</RelativeLayout>
